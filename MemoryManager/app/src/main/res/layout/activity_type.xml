<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity_Type">

    <Button
        android:id="@+id/button_anniversary"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="76dp"
        android:text="Anniversary"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/button_study"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="64dp"
        android:text="Study"
        app:layout_constraintTop_toBottomOf="@id/button_anniversary"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/button_common"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="64dp"
        android:text="Common"
        app:layout_constraintTop_toBottomOf="@id/button_study"
        tools:layout_editor_absoluteX="0dp" />

</androidx.constraintlayout.widget.ConstraintLayout>